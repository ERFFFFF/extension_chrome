{
  "name": "Chrome Extension adblocking",
  "description": "Chrome Extension used for adblocking.",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "accessibilityFeatures.modify",
    "accessibilityFeatures.read",
    "tabs",
    "https://www.google.com/*",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "notifications",
    "storage"
  ]
}